										{
											"referenceName": "ds_ecdcdata_raw_from_http",
											"type": "DatasetReference",
											"parameters": {
												"relativeURL": {
													"value": "@item().sourceRelativeURL",
													"type": "Expression"
												}
											}
										}
									],
									"outputs": [
										{
											"referenceName": "ds_ecdcdata_raw_to_datalake",
											"type": "DatasetReference",
											"parameters": {}
										}
									]
								}
							]
